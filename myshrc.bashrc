# Shortcut for navigating to the parent directory

alias parent='hop .. '
# Shortcut for navigating to the home directory
alias home='hop'

# List all files in long format with hidden files
alias la='reveal -la'

# Clear the terminal screen
alias c='clear'


alias e='exit'
alias hist="log"
alias histc="log purge"
alias reveall="reveal -l"

alias pl='proclore'